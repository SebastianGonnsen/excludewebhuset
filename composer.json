{
    "name": "sebastiangonnsen/excludewebhuset",
    "description": "This is my package excludewebhuset",
    "keywords": [
        "SebastianGonnsen",
        "excludewebhuset"
    ],
    "homepage": "https://github.com/sebastiangonnsen/excludewebhuset",
    "license": "MIT",
    "authors": [
        {
            "name": "SebastianGonnsen",
            "email": "sg@webhusetballum.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1"
    },
    "require-dev": {
        "pestphp/pest": "^2.20",
        "laravel/pint": "^1.0",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "SebastianGonnsen\\ExcludeWebhuset\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SebastianGonnsen\\ExcludeWebhuset\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}